 <template>\r\n  <ul>\r\n  <li>\r\n        <h4>我要充值</h4>\r\n      </li>\r\n      <li>\r\n<el-form ref=\"form\" :rules=\"rules\" :model=\"user_form\" label-width=\"80px\">\r\n  <el-form-item label=\"充值金额\">\r\n    <el-select v-model=\"amountVal\" placeholder=\"请选择充值金额\">\r\n      <el-option label=\"50元\" value=\"50\"></el-option>\r\n      <el-option label=\"100元\" value=\"100\"></el-option>\r\n    </el-select>\r\n  </el-form-item>\r\n  <el-form-item>\r\n    <el-button type=\"primary\" @click=\"onSubmit\">充值</el-button>\r\n    <el-button type=\"info\" @click=\"$router.push('/customer')\">取消</el-button>\r\n  </el-form-item>\r\n</el-form>\r\n</li>\r\n</ul>\r\n</template> -->\r\n<script>\r\nimport { runInThisContext } from \"vm\";\r\nimport request from \"../../utils/request\";\r\n\r\n  export default {\r\n    name:\"chongzhi\",\r\n    data() {\r\n      return {\r\n        user_form:{\r\n        },\r\n        tableData:[],\r\n        money:\"\"\r\n      }\r\n    },\r\n    created() {\r\n    let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.user_form = JSON.parse(str)\r\n    this.money=\"\"\r\n    this.load()\r\n  },\r\n    methods: {\r\n      load(){\r\n      request.get(\"/user\",{\r\n\r\n      }).then(res=>{\r\n        console.log(res)\r\n        this.tableData=res.data.records\r\n        this.total=res.data.total\r\n\r\n      })\r\n    },\r\n      onSubmit() {\r\n      this.user_form.account+=this.money;\r\n        console.log(this.user_form);\r\n      request.put(\"/user/chongzhi\",this.user_form,{params: {\r\n        money:this.money\r\n        }\r\n        }).then(res =>{\r\n        console.log(res)\r\n        if(res.code==='0'){\r\n          this.$message({\r\n            type:\"success\",\r\n            message:\"更新成功\"\r\n          })\r\n        }else {\r\n          this.$message({\r\n            type:\"error\",\r\n            message:res.msg\r\n          })\r\n        }\r\n        this.load()\r\n      })\r\n      },\r\n\r\n     \r\n    }\r\n  }\r\n\r\n</script> \r\n\r\n \r\n<style scoped>\r\n\t/* 信息列表样式 */\r\n\t.msg-box > li {\r\n\t\tlist-style: none;\r\n\t\tborder-bottom: 1px solid #c5c5c5;\r\n\t\tpadding: 20px 10px;\r\n\t}\r\n</style>"]}]}