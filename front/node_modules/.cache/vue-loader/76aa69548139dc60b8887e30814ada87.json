\2023-11-09-16-58-59-Springboot-Vue-Javaweb-Project-SCUT-master\\Springboot-Vue-Javaweb-Project-SCUT-master\\springboot-vue-demo\\vue\\src\\views\\chongzhi.vue"],"names":[],"mappings":";CA+IC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;CACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,EAAE;EACb,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;EAChB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EAChC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;CACnB","file":"E:/2023-11-09-16-58-59-Springboot-Vue-Javaweb-Project-SCUT-master/Springboot-Vue-Javaweb-Project-SCUT-master/springboot-vue-demo/vue/src/views/chongzhi.vue","sourceRoot":"","sourcesContent":["<!-- <template>\r\n<el-form ref=\"form\" :rules=\"rules\" :model=\"form\" label-width=\"80px\">\r\n  <el-form-item label=\"充值金额\">\r\n    <el-select v-model=\"form.money\" placeholder=\"请选择充值金额\">\r\n      <el-option label=\"50元\" value=\"50\"></el-option>\r\n      <el-option label=\"100元\" value=\"100\"></el-option>\r\n    </el-select>\r\n  </el-form-item>\r\n  <el-form-item>\r\n    <el-button type=\"primary\" @click=\"onSubmit\">充值</el-button>\r\n    <el-button type=\"info\" @click=\"$router.push('/customer')\">取消</el-button>\r\n  </el-form-item>\r\n</el-form>\r\n</template>\r\n<script>\r\nimport request from \"../../utils/request\";\r\n  export default {\r\n    data() {\r\n      return {\r\n        form: {\r\n          money: '',\r\n        }\r\n      }\r\n    },\r\n    methods: {\r\n      onSubmit() {\r\n        console.log('submit!');\r\n      request.put(\"/user\", this.form).then(res => {\r\n        console.log(res)\r\n        if (res.code === '0') {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"更新成功\"\r\n          })\r\n          sessionStorage.setItem(\"user\", JSON.stringify(this.form))\r\n          // 触发Layout更新用户信息\r\n          this.$emit(\"userInfo\")\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n      },\r\n     \r\n    }\r\n  }\r\n\r\n</script> -->\r\n<template>\r\n\t<el-card class=\"box-card\">\r\n\t\t<ul class=\"msg-box\">\r\n\t\t\t<li>\r\n\t\t\t\t<h1>充值</h1>\r\n\t\t\t</li>\r\n\t\t\t<li>\r\n\t\t\t\t<h4 style=\"margin-bottom: 15px;\">充值金额</h4>\r\n\t\t\t\t<el-radio-group v-model=\"form.account\" @change=\"amountChange\">\r\n\t\t\t\t\t<el-radio border :label=\"''+ 100\">充值100</el-radio>\r\n\t\t\t\t\t<el-radio border :label=\"''+ 500\">充值500</el-radio>\r\n\t\t\t\t\t<el-radio border :label=\"''+ 1000\">充值1000</el-radio>\r\n\t\t\t\t\t<el-radio border :label=\"''+ 2000\">充值2000</el-radio>\r\n\t\t\t\t\t<el-radio border :label=\"''+ 5000\">充值5000</el-radio>\r\n\t\t\t\t\t<el-radio border :label=\"''\">自定义</el-radio>\r\n\t\t\t\t</el-radio-group>\r\n\t\t\t</li>\r\n\t\t\t<li>\r\n\t\t\t\t<h4 style=\"margin-bottom: 15px;\">充值方式</h4>\r\n\t\t\t\t<el-radio-group v-model=\"rechargeParams.paymentType\" @change=\"paymentTypeChange\">\r\n\t\t\t\t\t<el-radio border :label=\"''+ 0\">微信</el-radio>\r\n\t\t\t\t\t<el-radio border :label=\"''+ 1\">支付宝</el-radio>\r\n\t\t\t\t</el-radio-group>\r\n\t\t\t</li>\r\n\t\t\t<li>\r\n\t\t\t\t<h4 style=\"margin-bottom: 15px;\">充值金额</h4>\r\n\t\t\t\t<el-input :disabled=\"disabled\" clearable v-model=\"rechargeParams.totalAmt\" placeholder=\"请输入金额\" style=\"width: 150px;\"></el-input>\r\n\t\t\t</li>\r\n\t\t</ul>\r\n\t\t<div style=\"text-align: center; margin-top: 30px;\">\r\n\t\t\t<el-button type=\"primary\" @click=\"surePay\">确认支付</el-button>\r\n\t\t</div>\r\n\t</el-card>\r\n</template>\r\n \r\n<script>\r\nimport request from \"../../utils/request\";\r\n\texport default {\r\n    name:\"chongzhi\",\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\tuser_form: {},\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\r\n    created() {\r\n    let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.user_form = JSON.parse(str)\r\n  },\r\n\t\tmethods: {\r\n\t\t\t//充值金额\r\n\t\t\tamountChange(val) {\r\n\t\t\t\tthis.rechargeParams.totalAmt = val;\r\n\t\t\t\tif (val == '') {\r\n\t\t\t\t\tthis.disabled = false\r\n\t\t\t\t} else {\r\n\t\t\t\t\tthis.disabled = true\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\t//支付方式\r\n\t\t\tpaymentTypeChange(val) {\r\n\t\t\t\tthis.rechargeParams.paymentType = val\r\n\t\t\t},\r\n\t\t\t//确认支付\r\n\t\t  surePay() {\r\n\t\t\t\tif (this.rechargeParams.totalAmt == '') {\r\n\t\t\t\t\tthis.$message.warning('请输入金额');\r\n\t\t\t\t\treturn;\r\n\t\t\t\t}\r\n        request.put(\"/user\", this.form).then(res => {\r\n        console.log(res)\r\n        if (res.code === '0') {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"更新成功\"\r\n          })\r\n          sessionStorage.setItem(\"user\", JSON.stringify(this.form))\r\n          // 触发Layout更新用户信息\r\n          this.$emit(\"userInfo\")\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n\t\t\t},\r\n\t\t\r\n\t\t}\r\n\t}\r\n</script>\r\n \r\n<style scoped>\r\n\t/* 信息列表样式 */\r\n\t.msg-box > li {\r\n\t\tlist-style: none;\r\n\t\tborder-bottom: 1px solid #c5c5c5;\r\n\t\tpadding: 20px 10px;\r\n\t}\r\n</style>\r\n"]}]}