a\" border stripe style=\"width: 100%\">\r\n      <el-table-column label=\"商品ID\" prop=\"id\" />\r\n      <el-table-column label=\"商户ID\" prop=\"storeId\"></el-table-column>\r\n      <el-table-column label=\"名称\" prop=\"name\" />\r\n      <el-table-column label=\"价格(¥)\" prop=\"price\" />\r\n      <!-- <el-table-column label=\"类别\" prop=\"category\" /> -->\r\n      <el-table-column label=\"类别\" prop=\"cateId\">\r\n        <template #default=\"scope\" >\r\n          <span v-for=\"index in category\">\r\n          <span v-if=\"scope.row.cateId===index.cateId\">\r\n            {{ index.catename }}\r\n          </span>\r\n        </span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"出厂时间\" prop=\"createTime\" />\r\n      <el-table-column label=\"购买数量\" prop=\"num\" />\r\n      <el-table-column\r\n          label=\"封面\">\r\n        <template #default=\"scope\">\r\n          <el-image\r\n              style=\"width: 100px; height: 100px\"\r\n              :src=\"scope.row.cover\"\r\n              :preview-src-list=\"[scope.row.cover]\"\r\n              :initial-index=\"1\"\r\n          >\r\n          </el-image>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"数量修改\" >\r\n        <template #default=\"scope\">\r\n          <el-input\r\n              v-model=\"scope.row.num\"\r\n              autocomplete=\"off\"\r\n              style=\"width: 80%\"></el-input>\r\n          <el-button\r\n              size=\"mini\"\r\n              @click=\"save(scope.row.num,scope.row.id)\"\r\n          >确认</el-button\r\n          >\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\">\r\n        <template #default=\"scope\">\r\n          <el-popconfirm title=\"你确定要提交订单嘛？\" @confirm=\"handleSubmit(scope.row.num,scope.row.id,scope.row.price,scope.row.storeId)\">\r\n            <template #reference>\r\n              <el-button\r\n                  size=\"mini\"\r\n              >支付并提交</el-button\r\n              >\r\n            </template>\r\n          </el-popconfirm>\r\n          <el-popconfirm title=\"你确定要删除嘛？\" @confirm=\"handleDelete(scope.row.num,scope.row.id)\">\r\n            <template #reference>\r\n              <el-button\r\n                  size=\"mini\"\r\n                  type=\"danger\"\r\n                  style=\"width: 70px\"\r\n              >删除</el-button\r\n              >\r\n            </template>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div class=\"demo-pagination-block\">\r\n      <!--    以下是显示页面码-->\r\n      <el-pagination\r\n          v-model:current-page=\"currentPage\"\r\n          :page-sizes=\"[10, 20, 25]\"\r\n          :page-size=\"10\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\"\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n      >\r\n      </el-pagination>\r\n      <!-- Form -->\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"../../utils/request\";\r\nexport default {\r\n  name: 'cart' ,\r\n  data() {\r\n    return {\r\n      dialogFormVisible:false,\r\n      form:{},\r\n      currentPage :1,\r\n      tableData: [\r\n      ],\r\n      category:[],\r\n      roleData:[],\r\n      search: '',\r\n      total:10,\r\n      dialogImageUrl: '',\r\n      user_form:{},\r\n    }\r\n  },\r\n  created() {\r\n    let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.user_form = JSON.parse(str)\r\n    this.form.userId=this.user_form.id\r\n    this.form.role=this.user_form.role\r\n    this.load()\r\n  },\r\n  methods: {\r\n    filesUploadSuccess(res,file){\r\n      console.log(res)\r\n      this.form.cover=res.data\r\n    },\r\n    load(){\r\n      request.get(\"/Cart\",{\r\n        params:{\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search,\r\n          id:this.user_form.id\r\n        }\r\n      }).then(res=>{\r\n        console.log(res)\r\n        this.tableData=res.data.records\r\n        this.total=res.data.total\r\n      })\r\n      request.get(\"/category\",{\r\n        }).then(res=>{\r\n          console.log(res)\r\n          this.category=res.data\r\n        })\r\n    },\r\n    //保存购物车修改\r\n    save(num,id){\r\n      this.form.num=num\r\n      this.form.storageId=id\r\n      console.log(this.form)\r\n      request.put(\"/Cart\",this.form).then(res =>{\r\n        console.log(res)\r\n        if(res.code==='0'){\r\n          this.$message({\r\n            type:\"success\",\r\n            message:\"更新成功\"\r\n          })\r\n        }else {\r\n          this.$message({\r\n            type:\"error\",\r\n            message:res.msg\r\n          })\r\n        }\r\n        this.load()\r\n      })\r\n\r\n    },\r\n\r\n    //提交订单\r\n    handleSubmit(num,id,price,sid){\r\n      this.form.num=num\r\n      this.form.storageId=id\r\n      this.form.customerId=this.user_form.id\r\n      this.form.storeId=sid\r\n      this.form.ostate=0\r\n      console.log(this.form)\r\n      request.post(\"/Order\",this.form, {params:{\r\n        id:this.user_form.id,\r\n        price:price\r\n\r\n      }\r\n        }).then(res =>{\r\n        console.log(res)\r\n        if(res.code==='0'){\r\n          this.$message({\r\n            type:\"success\",\r\n            message:\"提交订单成功\"\r\n          })\r\n        }else {\r\n          this.$message({\r\n            type:\"error\",\r\n            message:res.msg\r\n          })\r\n        }\r\n        this.load()\r\n      })\r\n\r\n      this.handleDelete(num,id)\r\n    },\r\n\r\n    //删除购物车\r\n    handleDelete(num,id) {\r\n      this.form.num=num\r\n      this.form.storageId=id\r\n      console.log(this.form)\r\n      request.post(\"/Cart/deleteEntity\",this.form).then(res=>{\r\n        if(res.code==='0'){\r\n          this.$message({\r\n            type:\"success\",\r\n            message:\"删除成功\"\r\n          })\r\n        }else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n        this.load()\r\n      })\r\n    },\r\n\r\n    //页面修改\r\n    handleSizeChange(pageSize){\r\n      this.pageSize=pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum){//改变页码\r\n      this.currentPage=pageNum\r\n      this.load()\r\n    }\r\n  },\r\n}\r\n</script>\r\n"],"mappings":";;EACOA,KAAiC,EAAjC;IAAA;EAAA;AAAiC;;;;;EAkE/BC,KAAK,EAAC;AAAuB;;;;;;;;;uBAlEpCC,mBAAA,CAgFM,OAhFNC,UAgFM,GA/EJC,YAAA,CAgEWC,mBAAA;IAhEAC,IAAI,EAAEC,KAAA,CAAAC,SAAS;IAAEC,MAAM,EAAN,EAAM;IAACC,MAAM,EAAN,EAAM;IAACV,KAAmB,EAAnB;MAAA;IAAA;;sBACxC,MAA0C,CAA1CI,YAAA,CAA0CO,0BAAA;MAAzBC,KAAK,EAAC,MAAM;MAACC,IAAI,EAAC;QACnCT,YAAA,CAA+DO,0BAAA;MAA9CC,KAAK,EAAC,MAAM;MAACC,IAAI,EAAC;QACnCT,YAAA,CAA0CO,0BAAA;MAAzBC,KAAK,EAAC,IAAI;MAACC,IAAI,EAAC;QACjCT,YAAA,CAA8CO,0BAAA;MAA7BC,KAAK,EAAC,OAAO;MAACC,IAAI,EAAC;QACpCC,mBAAA,wDAAuD,EACvDV,YAAA,CAQkBO,0BAAA;MARDC,KAAK,EAAC,IAAI;MAACC,IAAI,EAAC;;MACpBE,OAAO,EAAAC,QAAA,CAAEC,KAAK,M,kBACvBf,mBAAA,CAIKgB,SAAA,QAAAC,WAAA,CAJiBZ,KAAA,CAAAa,QAAQ,EAAjBC,KAAK;6BAAlBnB,mBAAA,CAIK,eAHOe,KAAK,CAACK,GAAG,CAACC,MAAM,KAAGF,KAAK,CAACE,MAAM,I,cAA3CrB,mBAAA,CAEO,QAAAsB,UAAA,EAAAC,gBAAA,CADFJ,KAAK,CAACK,QAAQ,oB;;;;QAKvBtB,YAAA,CAAkDO,0BAAA;MAAjCC,KAAK,EAAC,MAAM;MAACC,IAAI,EAAC;QACnCT,YAAA,CAA2CO,0BAAA;MAA1BC,KAAK,EAAC,MAAM;MAACC,IAAI,EAAC;QACnCT,YAAA,CAWkBO,0BAAA;MAVdC,KAAK,EAAC;IAAI;MACDG,OAAO,EAAAC,QAAA,CAAEC,KAAK,KACvBb,YAAA,CAMWuB,mBAAA;QALP3B,KAAmC,EAAnC;UAAA;UAAA;QAAA,CAAmC;QAClC4B,GAAG,EAAEX,KAAK,CAACK,GAAG,CAACO,KAAK;QACpB,kBAAgB,GAAGZ,KAAK,CAACK,GAAG,CAACO,KAAK;QAClC,eAAa,EAAE;;;QAKxBzB,YAAA,CAYkBO,0BAAA;MAZDC,KAAK,EAAC;IAAM;MAChBG,OAAO,EAAAC,QAAA,CAAEC,KAAK,KACvBb,YAAA,CAGkC0B,mBAAA;oBAFrBb,KAAK,CAACK,GAAG,CAACS,GAAG;yCAAbd,KAAK,CAACK,GAAG,CAACS,GAAG,GAAAC,MAAA;QACtBC,YAAY,EAAC,KAAK;QAClBjC,KAAkB,EAAlB;UAAA;QAAA;sEACJI,YAAA,CAIC8B,oBAAA;QAHGC,IAAI,EAAC,MAAM;QACVC,OAA