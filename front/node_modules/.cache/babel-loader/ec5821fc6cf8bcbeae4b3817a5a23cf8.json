\n        }\r\n      })\r\n      },\r\n     \r\n    }\r\n  }\r\n\r\n</script> -->\r\n<template>\r\n\t<el-card class=\"box-card\">\r\n\t\t<ul class=\"msg-box\">\r\n\t\t\t<li>\r\n\t\t\t\t<h1>充值</h1>\r\n\t\t\t</li>\r\n\t\t\t<li>\r\n\t\t\t\t<h4 style=\"margin-bottom: 15px;\">充值金额</h4>\r\n\t\t\t\t<el-radio-group v-model=\"form.account\" @change=\"amountChange\">\r\n\t\t\t\t\t<el-radio border :label=\"''+ 100\">充值100</el-radio>\r\n\t\t\t\t\t<el-radio border :label=\"''+ 500\">充值500</el-radio>\r\n\t\t\t\t\t<el-radio border :label=\"''+ 1000\">充值1000</el-radio>\r\n\t\t\t\t\t<el-radio border :label=\"''+ 2000\">充值2000</el-radio>\r\n\t\t\t\t\t<el-radio border :label=\"''+ 5000\">充值5000</el-radio>\r\n\t\t\t\t\t<el-radio border :label=\"''\">自定义</el-radio>\r\n\t\t\t\t</el-radio-group>\r\n\t\t\t</li>\r\n\t\t\t<li>\r\n\t\t\t\t<h4 style=\"margin-bottom: 15px;\">充值方式</h4>\r\n\t\t\t\t<el-radio-group v-model=\"rechargeParams.paymentType\" @change=\"paymentTypeChange\">\r\n\t\t\t\t\t<el-radio border :label=\"''+ 0\">微信</el-radio>\r\n\t\t\t\t\t<el-radio border :label=\"''+ 1\">支付宝</el-radio>\r\n\t\t\t\t</el-radio-group>\r\n\t\t\t</li>\r\n\t\t\t<li>\r\n\t\t\t\t<h4 style=\"margin-bottom: 15px;\">充值金额</h4>\r\n\t\t\t\t<el-input :disabled=\"disabled\" clearable v-model=\"rechargeParams.totalAmt\" placeholder=\"请输入金额\" style=\"width: 150px;\"></el-input>\r\n\t\t\t</li>\r\n\t\t</ul>\r\n\t\t<div style=\"text-align: center; margin-top: 30px;\">\r\n\t\t\t<el-button type=\"primary\" @click=\"surePay\">确认支付</el-button>\r\n\t\t</div>\r\n\t</el-card>\r\n</template>\r\n \r\n<script>\r\nimport request from \"../../utils/request\";\r\n\texport default {\r\n    name:\"chongzhi\",\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\tuser_form: {},\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\r\n    created() {\r\n    let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.user_form = JSON.parse(str)\r\n  },\r\n\t\tmethods: {\r\n\t\t\t//充值金额\r\n\t\t\tamountChange(val) {\r\n\t\t\t\tthis.rechargeParams.totalAmt = val;\r\n\t\t\t\tif (val == '') {\r\n\t\t\t\t\tthis.disabled = false\r\n\t\t\t\t} else {\r\n\t\t\t\t\tthis.disabled = true\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\t//支付方式\r\n\t\t\tpaymentTypeChange(val) {\r\n\t\t\t\tthis.rechargeParams.paymentType = val\r\n\t\t\t},\r\n\t\t\t//确认支付\r\n\t\t  surePay() {\r\n\t\t\t\tif (this.rechargeParams.totalAmt == '') {\r\n\t\t\t\t\tthis.$message.warning('请输入金额');\r\n\t\t\t\t\treturn;\r\n\t\t\t\t}\r\n        request.put(\"/user\", this.form).then(res => {\r\n        console.log(res)\r\n        if (res.code === '0') {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"更新成功\"\r\n          })\r\n          sessionStorage.setItem(\"user\", JSON.stringify(this.form))\r\n          // 触发Layout更新用户信息\r\n          this.$emit(\"userInfo\")\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n\t\t\t},\r\n\t\t\r\n\t\t}\r\n\t}\r\n</script>\r\n \r\n<style scoped>\r\n\t/* 信息列表样式 */\r\n\t.msg-box > li {\r\n\t\tlist-style: none;\r\n\t\tborder-bottom: 1px solid #c5c5c5;\r\n\t\tpadding: 20px 10px;\r\n\t}\r\n</style>\r\n"],"mappings":"AAsFA,OAAOA,OAAM,MAAO,qBAAqB;AACxC,eAAe;EACZC,IAAI,EAAC,UAAU;EACjBC,IAAIA,CAAA,EAAG;IACN,OAAO;MACNC,SAAS,EAAE,CAAC;IACZ;EACD,CAAC;EAEAC,OAAOA,CAAA,EAAG;IACV,IAAIC,GAAE,GAAIC,cAAc,CAACC,OAAO,CAAC,MAAM,KAAK,IAAG;IAC/C,IAAI,CAACJ,SAAQ,GAAIK,IAAI,CAACC,KAAK,CAACJ,GAAG;EACjC,CAAC;EACDK,OAAO,EAAE;IACR;IACAC,YAAYA,CAACC,GAAG,EAAE;MACjB,IAAI,CAACC,cAAc,CAACC,QAAO,GAAIF,GAAG;MAClC,IAAIA,GAAE,IAAK,EAAE,EAAE;QACd,IAAI,CAACG,QAAO,GAAI,KAAI;MACrB,OAAO;QACN,IAAI,CAACA,QAAO,GAAI,IAAG;MACpB;IACD,CAAC;IACD;IACAC,iBAAiBA,CAACJ,GAAG,EAAE;MACtB,IAAI,CAACC,cAAc,CAACI,WAAU,GAAIL,GAAE;IACrC,CAAC;IACD;IACCM,OAAOA,CAAA,EAAG;MACV,IAAI,IAAI,CAACL,cAAc,CAACC,QAAO,IAAK,EAAE,EAAE;QACvC,IAAI,CAACK,QAAQ,CAACC,OAAO,CAAC,OAAO,CAAC;QAC9B;MACD;MACIpB,OAAO,CAACqB,GAAG,CAAC,OAAO,EAAE,IAAI,CAACC,IAAI,CAAC,CAACC,IAAI,CAACC,GAAE,IAAK;QAC