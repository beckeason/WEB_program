1699529737028}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"names":["request","name","data","dialogFormVisible","form","currentPage","tableData","search","total","ruleForm","rules","account","required","message","trigger","min","max","created","load","methods","get","params","pageNum","pageSize","then","res","console","log","records","add","save","id","put","code","$message","type","msg","post","handleEdit","row","JSON","parse","stringify","handleDelete","delete","handleSizeChange","handleCurrentChange"],"sources":["E:\\2023-11-09-16-58-59-Springboot-Vue-Javaweb-Project-SCUT-master\\Springboot-Vue-Javaweb-Project-SCUT-master\\springboot-vue-demo\\vue\\src\\views\\User.vue"],"sourcesContent":["<template>\n  <div>\n    <!--    功能-->\n    <div style=\"margin:10px 0\">\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\n    </div>\n    <!--    搜索区域-->\n    <div style=\"margin:10px 0\">\n      <el-input v-model=\"search\" placeholder=\"请输入用户名\" style=\"width: 50%\" clearable></el-input>\n      <el-button type=\"primary\" style=\"margin-left:  5px\" @click=\"load\">查询</el-button>\n    </div>\n    <el-table :data=\"tableData\" border stripe style=\"width: 100%\">\n      <el-table-column label=\"ID\" prop=\"id\" />\n      <el-table-column label=\"用户名\" prop=\"username\" />\n      <el-table-column label=\"steam账号\" prop=\"steamname\" />\n      <el-table-column label=\"年龄\" prop=\"age\" />\n      <el-table-column label=\"性别\" prop=\"sex\" />\n      <el-table-column label=\"余额\" prop=\"account\" />\n      <el-table-column label=\"邮箱\" prop=\"email\" />\n      <el-table-column label=\"角色\">\n        <template #default=\"scope\">\n          <span v-if=\"scope.row.role===1\">顾客</span>\n          <span v-if=\"scope.row.role===2\">商家</span>\n          <span v-if=\"scope.row.role===3\">管理员</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\">\n        <template #default=\"scope\">\n          <el-button size=\"mini\" @click=\"handleEdit( scope.row)\"\n          >编辑</el-button\n          >\n          <el-popconfirm title=\"你确定要删除嘛？\" @confirm=\"handleDelete(scope.row.id)\">\n            <template #reference>\n              <el-button\n                  size=\"mini\"\n                  type=\"danger\"\n              >删除</el-button\n              >\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div class=\"demo-pagination-block\">\n      <el-pagination\n          v-model:current-page=\"currentPage\"\n          :page-sizes=\"[5, 10, 20, 25]\"\n          :page-size=\"10\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\"\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n      >\n      </el-pagination>\n\n\n      <!-- Form -->\n\n      <el-dialog v-model=\"dialogFormVisible\" title=\"添加\">\n        <el-form\n            :model=\"form\"\n            ref=\"ruleForm\"\n            label-width=\"120px\">\n          <el-form-item label=\"用户名\" >\n            <el-input v-model=\"form.username\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"昵称\" >\n            <el-input v-model=\"form.steamname\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"年龄\" >\n            <el-input v-model=\"form.age\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"性别\" >\n            <el-radio v-model=\"form.sex\" label=\"男\">男</el-radio>\n            <el-radio v-model=\"form.sex\" label=\"女\">女</el-radio>\n          </el-form-item>\n          <el-form-item prop=\"account\" label=\"余额\" >\n            <el-input v-model=\"form.account\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"邮箱地址\" >\n            <el-input v-model=\"form.email\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"角色\">\n            <el-radio v-model=\"form.role\" label=\"1\">顾客</el-radio>\n            <el-radio v-model=\"form.role\" label=\"2\">商家</el-radio>\n            <el-radio v-model=\"form.role\" label=\"3\">管理员</el-radio>\n          </el-form-item>\n\n        </el-form>\n        <template #footer>\n      <span class=\"dialog-footer\">\n        <el-button @click=\"dialogFormVisible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"save\"\n        >确认</el-button\n        >\n      </span>\n        </template>\n      </el-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport request from \"../../utils/request\";\n\nexport default {\n  name: 'user' ,\n  data() {\n    return {\n      dialogFormVisible:false,\n      form:{},\n      currentPage :1,\n      tableData: [\n      ],\n      search: '',\n      total:10,\n      ruleForm: {},\n      rules: {\n        account: [\n          {\n            required: true,\n            message: '请充值',\n            trigger: 'blur',\n          },\n          {\n            min: 0,\n            max: 8,\n            message: '长度在8八位数以内',\n            trigger: 'blur',\n          },\n        ],\n      },\n    }\n  },\n  created() {\n    this.load()\n  },\n  methods: {\n    load(){\n      request.get(\"/user\",{\n        params:{\n        pageNum: this.currentPage,\n        pageSize: this.pageSize,\n        search: this.search\n        }\n      }).then(res=>{\n        console.log(res)\n        this.tableData=res.data.records\n        this.total=res.data.total\n      })\n    },\n    add(){\n      this.dialogFormVisible = true;\n      this.form={};\n    },\n    save(){\n      if(this.form.id){\n        //update\n        request.put(\"/user\",this.form).then(res =>{\n          if(res.code==='0'){\n            this.$message({\n              type:\"success\",\n              message:\"更新成功\"\n            })\n          }else {\n            this.$message({\n              type:\"error\",\n              message:res.msg\n            })\n          }\n          this.load()\n          this.dialogFormVisible =false\n        })\n      }else{\n        request.post(\"/user\",this.form).then(res =>{\n          console.log(res)\n          if(res.code==='0'){\n            this.$message({\n              type:\"success\",\n              message:\"新增成功\"\n            })\n          }else {\n            this.$message({\n              type:\"error\",\n              message:res.msg\n            })\n          }\n          this.load()\n          this.dialogFormVisible=false\n        })\n      }\n    },\n    handleEdit(row) {\n      this.form=JSON.parse(JSON.stringify(row))\n      this.dialogFormVisible=true\n    },\n    handleDelete(id) {\n      console.log(id)\n      request.delete(\"/user/\"+id).then(res=>{\n        if(res.code==='0'){\n          this.$message({\n            type:\"success\",\n            message:\"删除成功\"\n          })\n        }else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n        this.load()\n      })\n    },\n    handleSizeChange(pageSize){\n      this.pageSize=pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum){//改变页码\n      this.currentPage=pageNum\n      this.load()\n    }\n  },\n}\n</script>\n"],"mappings":"AAuGA,OAAOA,OAAM,MAAO,qBAAqB;AAEzC,eAAe;EACbC,IAAI,EAAE,MAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,iBAAiB,EAAC,KAAK;MACvBC,IAAI,EAAC,CAAC,CAAC;MACPC,WAAU,EAAG,CAAC;MACdC,SAAS,EAAE,EACV;MACDC,MAAM,EAAE,EAAE;MACVC,KAAK,EAAC,EAAE;MACRC,QAAQ,EAAE,CAAC,CAAC;MACZC,KAAK,EAAE;QACLC,OAAO,EAAE,CACP;UACEC,QAAQ,EAAE,IAAI;UACdC,OAAO,EAAE,KAAK;UACdC,OAAO,EAAE;QACX,CAAC,EACD;UACEC,GAAG,EAAE,CAAC;UACNC,GAAG,EAAE,CAAC;UACNH,OAAO,EAAE,WAAW;UACpBC,OAAO,EAAE;QACX,CAAC;MAEL;IACF;EACF,CAAC;EACDG,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,IAA