<arrow-down /></el-icon>\n      </el-button>\n      <template #dropdown>\n        <el-dropdown-menu>\n        <el-dropdown-item v-for=\"index in category\" @click=\"handleClick(index.catename)\">{{index.catename}}</el-dropdown-item>\n        </el-dropdown-menu>\n      </template>\n    </el-dropdown>\n    <!--    搜索区域-->\n      <el-input v-model=\"search\" placeholder=\"请输入关键字\" style=\"width: 20%;border-bottom-color: black\" clearable></el-input>\n      <el-button type=\"primary\" style=\"margin-left:  10px ;background-color: #337ecc; color: white;border-color: white;\" @click=\"load\">查询</el-button>\n    \n    <!-- <el-sub-menu index=\"category\" v-if=\" user.role===1\">\n          <template #title>\n            <el-icon><more /></el-icon>\n            <span>类别</span>\n          </template>\n            <el-menu-item  v-for=\"(o, index) in tableData.length\"\n            :key=\"o\"\n            :span=\"110\"\n            :offset=\"1\">\n            <span slot=\"title\">{{ tableData[index].catename }}</span>\n            </el-menu-item>\n          </el-sub-menu> -->\n     <el-carousel :interval=\"4000\" type=\"card\" height=\"200px\">\n    <el-carousel-item  v-for=\"(o, index) in tableData.length\"\n            :key=\"o\"\n            :span=\"110\" \n            :offset=\"1\">\n      <h3 text=\"2xl\" justify=\"center\"><el-image\n                style=\"width: 300px; height:200px\"\n                :src=\"tableData[index].cover\"\n                fit=\"cover\"\n                class=\"image\"\n            /></h3>\n    </el-carousel-item>\n  </el-carousel> \n    <div>\n      <el-row>\n        <el-col\n            v-for=\"(o, index) in tableData.length\"\n            :key=\"o\"\n            :span=\"110\"\n            :offset=\"1\"\n        >\n          <el-card\n              style=\"width: 300px; height:430px\"\n\n              :body-style=\"{ padding: '0px' }\">\n            <el-image\n                style=\"width: 300px; height:300px\"\n                :src=\"tableData[index].cover\"\n                fit=\"cover\"\n                class=\"image\"\n            />\n            <div style=\"padding: 14px\">\n              <span style=\"font-weight: bold;\" >{{tableData[index].name}}</span>\n              <div style=\"color:gray;\">{{tableData[index].price+\"元\"}}</div>\n              <div class=\"bottom\">\n                <el-button plain @click=\"handleEdit( tableData[index])\" >查看详情</el-button>\n              </div>\n            </div>\n          </el-card>\n        </el-col>\n      </el-row>\n    </div>\n\n\n    <div>\n      <el-row>\n        <!--    以下是显示页面码-->\n        <el-pagination\n            v-model:current-page=\"currentPage\"\n            :page-sizes=\"[10, 20, 25]\"\n            :page-size=\"10\"\n            layout=\"total, sizes, prev, pager, next, jumper\"\n            :total=\"total\"\n            @size-change=\"handleSizeChange\"\n            @current-change=\"handleCurrentChange\"\n        >\n        </el-pagination>\n      </el-row>\n      <el-dialog v-model=\"dialogFormVisible\" title=\"添加\">\n        <el-form\n            :rules=\"rules\"\n            :model=\"form\"\n            label-width=\"120px\">\n          <el-form-item >\n            <el-image\n                style=\"width: 300px; height:300px\"\n                :src=\"form.cover\"\n                fit=\"cover\"\n                class=\"image\"\n            />\n          </el-form-item>\n          <el-form-item label=\"名称\" >\n            <el-input disabled v-model=\"form.name\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"价格\" >\n            <el-input disabled v-model=\"form.price\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"类别\" >\n            <el-input disabled v-model=\"form.category\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"出厂时间\" >\n            <el-date-picker disabled v-model=\"form.createTime\" fvalue-format=\"YYYY-MM-DD\" type=\"date\" style=\"width: 80%\" clearable></el-date-picker>\n          </el-form-item>\n          <el-form-item prop=\"num\" label=\"购买数量\" >\n            <el-input\n                v-model=\"form.num\"\n                autocomplete=\"off\"\n                style=\"width: 80%\"></el-input>\n          </el-form-item>\n        </el-form>\n        <template #footer>\n      <span class=\"dialog-footer\">\n        <el-button @click=\"dialogFormVisible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"AddToCard\" style=\"margin-left:  5px ;background-color: black; color: white;border-color: white;\"\n        >放入购物车</el-button\n        >\n      </span>\n        </template>\n      </el-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport request from \"../../utils/request\";\nimport { UploadFilled } from '@element-plus/icons'\nimport { ArrowDown } from '@element-plus/icons-vue'\n\nexport default {\n  name: 'customer' ,\n  components:{\n    UploadFilled,\n  },\n\n  data() {\n    return {\n      dialogFormVisible:false,\n      drawer:false,\n      form:{\n      },\n      currentPage :1,\n      tableData: [\n      ],\n      category:[],\n      rules: {\n        num: [\n          {\n            required: true,\n            message: '请输入数量',\n            trigger: 'blur',\n          },\n        ],\n      },\n      search: '',\n      total:10,\n      dialogImageUrl: '',\n      shoppingCart:{\n      },\n      user:{},\n      log_form:{},\n    }\n  },\n  created() {\n    let str = sessionStorage.getItem(\"user\")\n    this.user = JSON.parse(str)\n    this.load()\n  },\n  methods: {\n    filesUploadSuccess(res,file){\n      console.log(res)\n      this.form.cover=res.data\n\n    },\n    load(){\n      console.log(this.search)\n      request.get(\"/goods\",{\n        params:{\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res=>{\n        console.log(res)\n        this.tableData=res.data.records\n        this.total=res.data.total\n        \n      })\n      request.get(\"/category\",{\n    \n      }).then(res=>{\n        console.log(res)\n        this.category=res.data\n\n        \n      })\n    },\n    AddToCard(){\n      if(this.form.id) {\n        this.shoppingCart.userId = this.user.id+ ''\n        this.shoppingCart.storageId = this.form.id+ ''\n        this.shoppingCart.num = this.form.num\n        this.shoppingCart.role = this.user.role\n        //update\n        console.log(\"start\")\n        console.log(this.shoppingCart)\n        console.log(\"end\")\n        request.post(\"/Cart\", this.shoppingCart).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"已加入购物车\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n          this.load()\n          this.dialogFormVisible = false\n        })\n      }\n    },\n\n    detail(row){\n      this.form=JSON.parse(JSON.stringify(row))\n      this.dialogFormVisible=true\n      this.$nextTick(()=>{\n        this.$refs['upload'].clearFiles()\n      })\n    },\n    handleClick(name) {\n    this.search = name;\n    this.load();\n  },\n    handleEdit(row) {\n      this.form=JSON.parse(JSON.stringify(row))\n      this.form.num=1\n      this.dialogFormVisible=true\n      this.log_form.userId=this.user.id+ ''\n      this.log_form.storageId=this.form.id+ ''\n      this.log_form.action=\"浏览\"\n      console.log(this.log_form)\n      request.post(\"/logController\",this.log_form).then(res => {\n        console.log(res)\n        if (res.code === '0') {\n          this.$message({\n            type: \"成功\",\n            message: \"浏览日志保存成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n      })\n    },\n    handleSizeChange(pageSize){\n      this.pageSize=pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum){//改变页码\n      this.currentPage=pageNum\n      this.load()\n    }\n  },\n}\n</script>\n\n\n<style scoped>\n.el-row {\n  margin-bottom: 20px;\n}\n.el-col {\n  border-radius: 4px;\n}\n.bottom {\n  margin-top: 13px;\n  line-height: 12px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.button {\n  padding: 0;\n  min-height: auto;\n}\n\n.image {\n  width: 100%;\n  display: block;\n}\n.el-carousel__item h3 {\n  color: #475669;\n  opacity: 0.75;\n  line-height: 200px;\n  margin: 0;\n  text-align: center;}\n  .el-carousel__item:nth-child(2n) {\n  background-color: #99a9bf;\n}\n\n.el-carousel__item:nth-child(2n + 1) {\n  background-color: #d3dce6;\n}\n.example-showcase .el-dropdown + .el-dropdown {\n  margin-left: 15px;\n}\n.example-showcase .el-dropdown-link {\n  cursor: pointer;\n  color: white;\n  display: flex;\n  align-items: center;\n}\n</style>"],"mappings":"AAoIA,OAAOA,OAAM,MAAO,qBAAqB;AACzC,SAASC,YAAW,QAAS,qBAAoB;AACjD,SAASC,SAAQ,QAAS,yBAAwB;AAElD,eAAe;EACbC,IAAI,EAAE,UAAS;EACfC,UAAU,EAAC;IACTH;EACF,CAAC;EAEDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,iBAAiB,EAAC,KAAK;MACvBC,MAAM,EAAC,KAAK;MACZC,IAAI,EAAC,CACL,CAAC;MACDC,WAAU,EAAG,CAAC;MACdC,SAAS,EAAE,EACV;MACDC,QAAQ,EAAC,EAAE;MACXC,KAAK,EAAE;QACLC,GAAG,EAAE,CACH;UACEC,QAAQ,EAAE,IAAI;UACdC,OAAO,EAAE,OAAO;UAChBC,OAAO,EAAE;QACX,CAAC;MAEL,CAAC;MACDC,MAAM,EAAE,EAAE;MACVC,KAAK,EAAC,EAAE;MACRC,cAAc,EAAE,EAAE;MAClBC,YAAY,EAAC,CACb,CAAC;MACDC,IAAI,EAAC,CAAC,CAAC;MACPC,QAAQ,EAAC,CAAC;IACZ;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAIC,GAAE,GAAIC,cAAc,CAACC,OAAO,CAAC,MAAM;IACvC,IAAI,CAACL,IAAG,GAAIM,IAAI,CAACC,KAAK,CAACJ,GAAG;IAC1B,IAAI,CAACK,IAAI,CAAC;EACZ,CAAC;EACDC,OAAO,EAAE;IACPC,kBAAkBA,CAACC,GAAG,EAACC,IAAI,EAAC;MAC1BC,OAAO,CAACC,GAAG,CAACH,GAAG;MACf,IAAI,CAACxB,IA