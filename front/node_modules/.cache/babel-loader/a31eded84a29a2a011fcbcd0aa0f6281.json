r\", JSON.stringify(res.data))  // 缓存用户信息\n              if (res.data.role===1)\n              {\n                this.$router.push(\"/customer\")//跳转到用户\n              }else if(res.data.role===2) {\n                this.$router.push(\"/goods\")//跳转到主页}\n              }else {\n                this.$router.push(\"/user\")\n              }\n\n            }else{\n              this.$message({\n                type: \"error\",\n                message: res.msg\n              })\n            }\n          })\n        } else {\n          console.log('error submit!!')\n          return false\n        }\n      })\n    },\n  },\n}\n</script>\n\n<style scoped>\n\n</style>"],"mappings":";;EACOA,KAA2E,EAA3E;IAAA;IAAA;IAAA;IAAA;EAAA;AAA2E;;EACzEA,KAAsC,EAAtC;IAAA;IAAA;EAAA;AAAsC;gCACzCC,mBAAA,CAEW;EADPD,KAA4E,EAA5E;IAAA;IAAA;IAAA;IAAA;EAAA;AAA4E,GAC/E,MAAI;gCACLC,mBAAA,CAEM;EAFDD,KAAwE,EAAxE;IAAA;IAAA;IAAA;IAAA;EAAA;AAAwE,GAAE,MAE/E;;;;;;;uBAPJE,mBAAA,CAgDM,OAhDNC,UAgDM,GA/CJF,mBAAA,CA8CM,OA9CNG,UA8CM,GA7CJC,UAEW,EACXC,UAEM,EACNC,YAAA,CAsCUC,kBAAA;IArCNC,GAAG,EAAC,UAAU;IACbC,KAAK,EAAEC,KAAA,CAAAC,QAAQ;IAChB,aAAW,EAAX,EAAW;IACVC,KAAK,EAAEF,KAAA,CAAAE,KAAK;IACbC,IAAI,EAAC,QAAQ;IACb,aAAW,EAAC,MAAM;IAClBd,KAAQ,EAAR,EAAQ;IACRe,KAAK,EAAC;;sBAER,MAQe,CARfR,YAAA,CAQeS,uBAAA;MARCC,IAAI,EAAC;IAAU;wBAC7B,MAMY,CANZV,YAAA,CAMYW,mBAAA;QALR,aAAW,EAAC,oBAAoB;oBACvBP,KAAA,CAAAC,QAAQ,CAACO,QAAQ;mEAAjBR,KAAA,CAAAC,QAAQ,CAACO,QAAQ,GAAAC,MAAA;QAC1BpB,KAAyC,EAAzC;UAAA;UAAA;QAAA,CAAyC;QACzCqB,IAAI,EAAC,UAAU;QACfC,YAAY,EAAC;;;QAGnBf,YAAA,CAQeS,uBAAA;MARCC,IAAI,EAAC;IAAU;wBAC7B,MAMY,CANZV,YAAA,CAMYW,mBAAA;QALR,aAAW,EAAC,mBAAmB;QAC/BlB,KAAyC,EAAzC;UAAA;UAAA;QAAA,CAAyC;oBAChCW,KAAA,CAAAC,QAAQ,CAACW,QAAQ;mEAAjBZ,KAAA,CAAAC,QAAQ,CAACW,QAAQ,GAAAH,MAAA;QAC1BC,IAAI,EAAC,UAAU;QACfC,YAAY,EAAC;;;QAGnBf,YAAA,CAIeS,uBAAA;wBAHb,MAEC,CAFDT,YAAA,CAECiB,oBAAA;QAFUxB,KAA+F,EAA/F;UAAA;UAAA;UAAA;UAAA;UAAA;QAAA,CAA+F;QAACqB,IAAI,EAAC,SAAS;QAAGI,OAAK,EAAEC,QAAA,CAAAC;;0BAClI,MAAE,C,iBAAF,IAAE,E;;;;QAGLpB,YAAA,CAIeS,uBAAA;wBA