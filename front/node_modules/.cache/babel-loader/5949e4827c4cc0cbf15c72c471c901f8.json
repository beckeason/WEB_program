ault=\"scope\">\n            <el-popconfirm v-if=\"scope.row.ostate===0\" title=\"你确定要发货嘛？\" @confirm=\"save(scope.row.num,scope.row.id,scope.row.orderId,scope.row.price,scope.row.storeId,scope.row.customerId,scope.row)\">\n              <template #reference>\n                <el-button\n                    size=\"mini\"\n                    type=\"danger\"\n                >发货</el-button\n                >\n              </template>\n            </el-popconfirm>\n            <el-button  disabled v-if=\"scope.row.ostate===1\">\n              已发货\n            </el-button>\n            <el-button  disabled v-if=\"scope.row.ostate===2\">\n              已确认收货\n            </el-button>\n          </template>\n        </el-table-column>\n        <el-table-column v-if=\"user_form.role===1\" label=\"确认收货\">\n          <template #default=\"scope\">\n            <el-button  disabled v-if=\"scope.row.ostate===0\">\n              待发货\n            </el-button>\n            <el-popconfirm v-if=\"scope.row.ostate===1\" title=\"你确定收货嘛？\" @confirm=\"save(scope.row.num,scope.row.id,scope.row.orderId,scope.row.price,scope.row.storeId,scope.row.customerId,scope.row)\">\n              <template #reference>\n                <el-button\n                    size=\"mini\"\n                    type=\"danger\"\n                >收货</el-button\n                >\n              </template>\n            </el-popconfirm>\n            <el-button  disabled v-if=\"scope.row.ostate===2\">\n              已确认收货\n            </el-button>\n          </template>\n        </el-table-column>\n    </el-table>\n    <div class=\"demo-pagination-block\">\n      <!--    以下是显示页面码-->\n      <el-pagination\n          v-model:current-page=\"currentPage\"\n          :page-sizes=\"[10, 20, 25]\"\n          :page-size=\"10\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\"\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n      >\n      </el-pagination>\n    </div>\n  </div>\n</template>\n\n<script>\nimport request from \"../../utils/request\";\n\nexport default {\n  name: 'order' ,\n  data() {\n    return {\n      dialogFormVisible:false,\n      form:{},\n      currentPage :1,\n      tableData: [\n      ],\n      category:[],\n      roleData:[],\n      search: '',\n      total:10,\n      dialogImageUrl: '',\n      user:0,\n      user_form:{},\n      Visible:false,\n      customer_form:{},\n      storage_form:{},\n      log_form:{},\n    }\n  },\n  created() {\n    let str = sessionStorage.getItem(\"user\") || \"{}\"\n    this.user_form = JSON.parse(str)\n    this.form.customerId=this.user_form.id\n    this.form.role=this.user_form.role\n    this.load()\n  },\n  methods: {\n    filesUploadSuccess(res,file){\n      console.log(res)\n      this.form.cover=res.data\n    },\n    load(){\n      if(this.user_form.role===1){\n        request.get(\"/Order\",{\n          params:{\n            pageNum: this.currentPage,\n            pageSize: this.pageSize,\n            search: this.search,\n            id:this.user_form.id\n          }\n        }).then(res=>{\n          console.log(res)\n          this.tableData=res.data.records\n          this.total=res.data.total\n        })\n        request.get(\"/category\",{\n        }).then(res=>{\n          console.log(res)\n          this.category=res.data\n        })\n      }else {\n        console.log(\"store\")\n        request.get(\"/Order/store\",{\n          params:{\n            pageNum: this.currentPage,\n            pageSize: this.pageSize,\n            search: this.search,\n            id:this.user_form.id\n          }\n        }).then(res=>{\n          this.tableData=res.data.records\n          this.total=res.data.total\n        })\n        request.get(\"/category\",{\n        }).then(res=>{\n          console.log(res)\n          this.category=res.data\n        })\n      }\n\n    },\n    add(){\n      this.dialogFormVisible = true;\n      this.form={};\n      this.$refs['upload'].clearFiles()\n    },\n    save(num,id,orderId,price,sid,cid,row){\n      this.storage_form=JSON.parse(JSON.stringify(row))\n      console.log(this.storage_form)\n      this.form.num=num\n      this.form.storageId=id\n      this.form.customerId=cid\n      if(this.user_form.role===1){\n        this.form.ostate=2\n      }else {\n        this.form.ostate=1\n        //邮件确认\n        request.get(\"/user/getCustomer\",\n        {params:{id:this.form.customerId}}).then(res=>{\n          this.customer_form=res.data\n          this.storage_form.email=this.customer_form.email\n          this.storage_form.username=this.customer_form.username\n          console.log(this.storage_form)\n          request.post(\"/send\",\n              this.storage_form\n        ).then(res2 =>{\n            if(res2.code==='0'){\n              this.$message({\n                type:\"success\",\n                message:\"邮件确认成功\"\n              })\n            }else {\n              this.$message({\n                type:\"error\",\n                message:res2.msg\n              })\n            }\n          })\n        })\n      }\n      this.form.id=orderId\n      this.form.storeId=sid\n      console.log(this.form)\n      request.put(\"/Order\",this.form,{params: {\n          id: this.user_form.id,\n          price: price\n        }\n        }).then(res =>{\n        console.log(res)\n        if(res.code==='0'){\n          this.$message({\n            type:\"success\",\n            message:\"更新成功\"\n          })\n        }else {\n          this.$message({\n            type:\"error\",\n            message:res.msg\n          })\n        }\n        this.load()\n      })\n      if(this.user_form.role===1) {\n        this.log_form.userId = this.user_form.id+ ''\n        this.log_form.storageId = this.storage_form.id+''\n        this.log_form.action = \"购买\"\n        request.post(\"/logController\", this.log_form).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"购买日志保存成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n        })\n      }\n\n    },\n\n    handleSizeChange(pageSize){\n      this.pageSize=pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum){//改变页码\n      this.currentPage=pageNum\n      this.load()\n    }\n  },\n}\n</script>\n"],"mappings":";;EACOA,KAAiC,EAAjC;IAAA;EAAA;AAAiC;;;;;;;;;;;;;;EA+E/BC,KAAK,EAAC;AAAuB;;;;;;;;uBA/EpCC,mBAAA,CA4FM,OA5FNC,UA4FM,GA3FJC,YAAA,CA6EWC,mBAAA;IA7EAC,IAAI,EAAEC,KAAA,CAAAC,SAAS;IAAEC,MAAM,EAAN,EAAM;IAACC,MAAM,EAAN,EAAM;IAACV,KAAmB,EAAnB;MAAA;IAAA;;sBACxC,MAA+C,CAA/CI,YAAA,CAA+CO,0BAAA;MAA9BC,KAAK,EAAC,MAAM;MAACC,IAAI,EAAC;QACnCT,YAAA,CAAkDO,0BAAA;MAAjCC,KAAK,EAAC,MAAM;MAACC,IAAI,EAAC;QACnCT,YAAA,CAA0CO,0BAAA;MAAzBC,KAAK,EAAC,MAAM;MAACC,IAAI,EAAC;QACnCT,YAAA,CAA+DO,0BAAA;MAA9CC,KAAK,EAAC,MAAM;MAACC,IAAI,EAAC;QACnCT,YAAA,CAA0CO,0BAAA;MAAzBC,KAAK,EAAC,IAAI;MAACC,IAAI,EAAC;QACjCT,YAAA,CAA8CO,0BAAA;MAA7BC,KAAK,EAAC,OAAO;MAACC,IAAI,EAAC;QACpCC,mBAAA,wDAAuD,EACvDV,YAAA,CAQkBO,0BAAA;MARDC,KAAK,EAAC,IAAI;MAACC,IAAI,EAAC;;MACpBE,OAAO,EAAAC,QAAA,CAAEC,KAAK,M,kBACvBf,mBAAA,CAIKgB,SAAA,QAAAC,WAAA,CAJiBZ,KAAA,CAAAa,QAAQ,EAAjBC,KAAK;6BAAlBnB,mBAAA,CAIK,eAHOe,KAAK,CAACK,GAAG,CAACC,MAAM,KAAGF,KAAK,CAACE,MAAM,I,cAA3CrB,mBAAA,CAEO,QAAAsB,UAAA,EAAAC,gBAAA,CADFJ,KAAK,CAACK,QAAQ,oB;;;;QAKvBtB,YAAA,CAAkDO,0BAAA;MAAjCC,KAAK,EAAC,MAAM;MAACC,IAAI,EAAC;QACnCT,YAAA,CAA2CO,0BAAA;MAA1BC,KAAK,EAAC,MAAM;MAACC,IAAI,EAAC;QACnCT,YAAA,CAWkBO,0BAAA;MAVdC,KAAK,EAAC;IAAI;MACDG,OAAO,EAAAC,QAAA,CAAEC,KAAK,KACvBb,YAAA,CAMWuB,mBAAA;QALP3B,KAAmC,EAAnC;UAAA;UAAA;QAAA,CAAmC;QAClC4B,GAAG,EAAEX,KAAK,CAACK,GAAG,CAACO,KAAK;QACpB,kBAAgB,GAAGZ,KAAK,CAACK,GAAG,CAACO,KAAK;QAClC,eAAa,EAAE;;;QAKxBzB,YAAA,CAMkBO,0BAAA;MANDC,KAAK,EAAC;IAAM;MAChBG,OAAO,EAAAC,QAAA,CAAEC,KAAK,KACXA,KAAK,CAACK,GAAG,CAACQ,MAAM,U,